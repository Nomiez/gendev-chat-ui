{
  "name": "gendev-chat-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "generateAPI": "rimraf src/api && openapi-generator-cli generate -i ../server/openapi.yaml --additional-properties=supportsES6=true,withSeparateModelsAndApi=true,apiPackage=apis,modelPackage=models -g typescript-axios -o ./src/api",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/themes": "^2.0.0",
    "preact": "^10.16.0",
    "radix-ui": "^1.0.1",
    "react-router-dom": "^6.18.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "@preact/preset-vite": "^2.5.0",
    "@types/react-router-dom": "^5.3.3",
    "rimraf": "^5.0.5",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "yarn": "^1.22.19"
  }
}
